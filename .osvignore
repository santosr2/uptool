dist/
build/
*.egg-info/

# Test fixtures and vendored code
testdata/
vendor/

# Documentation build artifacts
site/
docs/api/
docs/cli/

# Cache directories
cache/
.cache/

# Node modules (if present)
node_modules/

# Python virtual environments
venv/
.venv/
env/
.env/
